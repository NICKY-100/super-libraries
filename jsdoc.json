{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": ".",
        "includePattern": "\\.js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    }
}